<div class="content podcasts">
    <h2 class="title-2">Podcasts relacionados</h2>

    <ul class="podcasts__list">
        {% for element in content.podcast %}
            <li class="podcasts__item">
                <audio src="{{ element.url }}"
                       controls
                >
                    Your browser does not support the audio element.
                </audio>
            </li>
        {% endfor %}
    </ul>
</div>